@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix adms: <http://www.w3.org/ns/adms#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix ofd: <https://w3id.org/openfooddata/core#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vf: <https://w3id.org/valueflows/> .
@prefix schema: <https://schema.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core> .
@prefix wikibase: <http://wikiba.se/ontology#> .
@prefix agrovoc: <http://aims.fao.org/aos/agrovoc> .

<https://w3id.org/openfooddata/core> a owl:Ontology, adms:SemanticAsset, skos:ConceptScheme ;
    dcterms:title "Open Food Data Ontology" ;
    dcterms:description "This ontology models the Open Food Data domain. It allows to describe food products, food producers/farmers, food shops/hubs and other actors and collaboration agreements between those" ;
    dc:license "https://creativecommons.org/publicdomain/" ;
    dcterms:modified "2017-02-17"^^xsd:date;
    vann:preferredNamespaceUri "https://w3id.org/openfooddata/core" ;
    vann:preferredNamespacePrefix "ofd" ;
    foaf:homepage <https://w3id.org/openfooddata/core> ;
    dcterms:created "2017-02-17"^^xsd:date ;
    dcterms:publisher ofd:OFD ;
    dcterms:partOf <https://w3id.org/openfooddata/> ;
    dcterms:type <http://purl.org/adms/assettype/Ontology> ;
    dcterms:status <http://purl.org/adms/status/UnderDevelopment> ;
    dc:creator ofd:Kalin .

ofd:OFD a dcterms:Agent ;
    foaf:member ofd:Kalin ;
    foaf:name "Open Food Data" ;
    foaf:homepage <http://www.openfooddata.org/> .

ofd:Kalin a foaf:Person ;
    foaf:name "Kalin Maldzhanski" ;
    foaf:homepage <http://djodjo.org> .


############## Interop

ofd:broaderAgrovoc a owl:ObjectProperty ;
    rdfs:label "broaderAgrovoc" ;
    rdfs:subPropertyOf skos:related ;
    rdfs:domain skos:Concept ;
    rdfs:range skos:Concept .

ofd:broaderWikidata a owl:ObjectProperty ;
    rdfs:label "broaderWikidata" ;
    rdfs:domain skos:Concept ;
    rdfs:range wikibase:Item .

ofd:broaderDbpedia a owl:ObjectProperty ;
    rdfs:label "broaderDbpedia" ;
    rdfs:domain skos:Concept ;
    rdfs:range owl:Thing .

ofd:broaderSchemaOrg a owl:ObjectProperty ;
    rdfs:label "broaderSchemaOrg" ;
    rdfs:domain skos:Concept ;
    rdfs:range schema:Thing .

############## base classes

# to be used to build up non-strict, reusable Food Type hierarchies. simple mostly linked to common sources, ex: potato

ofd:FoodType a owl:Class ;
    rdfs:label "Food Type" ;
    rdfs:comment "base class for hierarchical classification of a Food Product/Resource" ;
    rdfs:subClassOf skos:Concept , vf:Category .

# TODO link with vf:Actor
ofd:FoodActorType a owl:Class ;
    rdfs:label "Food Actor" ;
    rdfs:subClassOf skos:Concept .

# to be used to build up non-strict, reusable Food product hierarchies
# TODO link with vf:ResourceMaterialItemTemplate when ready
ofd:FoodProductType a owl:Class ;
    rdfs:label "Food Product" ;
    rdfs:comment "base class for offered Food Products" ;
    rdfs:subClassOf skos:Concept .

# TODO link with vf:ServiceTemplate
ofd:FoodServiceType a owl:Class ;
    rdfs:label "Food Service" ;
    rdfs:comment "base class for offered Food Service" ;
    rdfs:subClassOf skos:Concept  .

# these might be anything that is used in the processes, ex: glass/plastic container, paper/plastic bag, cold box, printing paper, etc.
# TODO link with vf:ResourceMaterialItemTemplate when ready
ofd:NonFoodItemType a owl:Class ;
    rdfs:label "Non-Food Item" ;
    rdfs:subClassOf skos:Concept .

####################### base properties

# links Types to Products
ofd:broaderFoodType a owl:ObjectProperty ;
    rdfs:label "broaderFoodType" ;
    rdfs:subPropertyOf skos:related ;
    rdfs:domain ofd:FoodProductType ;
    rdfs:range ofd:FoodType .

ofd:narrowerFoodProduct a owl:ObjectProperty ;
    rdfs:label "narrowerFoodProduct" ;
    rdfs:subPropertyOf skos:related ;
    owl:inverseOf ofd:broaderFoodType ;
    rdfs:domain ofd:FoodType ;
    rdfs:range ofd:FoodProductType.


ofd:actorType a owl:ObjectProperty ;
    rdfs:label "actorType" ;
    rdfs:domain foaf:Agent ;
    rdfs:range ofd:FoodActorType .

ofd:offersProduct a owl:ObjectProperty ;
    rdfs:label "offersProduct" ;
    rdfs:domain foaf:Agent ;
    rdfs:range ofd:FoodProductType.

ofd:offersService a owl:ObjectProperty ;
    rdfs:label "offersService" ;
    rdfs:domain foaf:Agent ;
    rdfs:range ofd:FoodServiceType .

ofd:offersNonFoodItem a owl:ObjectProperty ;
    rdfs:label "offersNonFoodItem" ;
    rdfs:domain foaf:Agent ;
    rdfs:range ofd:NonFoodItemType .

ofd:producedBy a owl:ObjectProperty ;
    rdfs:label "producedBy" ;
    rdfs:domain ofd:FoodProductType;
    rdfs:range foaf:Agent .

# the list of products the domain is made form
ofd:madeOf a owl:ObjectProperty ;
    rdfs:subPropertyOf skos:related ;
    owl:inverseOf ofd:usedIn ;
    rdfs:label "madeOf" ;
    rdfs:domain ofd:FoodProductType;
    rdfs:range ofd:FoodProductType.

# reverse of madeOf
ofd:usedIn a owl:ObjectProperty ;
    rdfs:subPropertyOf skos:related ;
    owl:inverseOf ofd:madeOf ;
    rdfs:label "usedIn" ;
    rdfs:domain ofd:FoodProductType;
    rdfs:range ofd:FoodProductType.

# TODO could possibly use/link VF vocabs relation...
ofd:cooperatesWith a skos:Concept, owl:TransitiveProperty, owl:SymmetricProperty ;
    rdfs:label "cooperatesWith" ;
    ofd:relatedService ofd:FoodService ;
    rdfs:domain foaf:Agent ;
    rdfs:range foaf:Agent .

ofd:deliversTo a skos:Concept, owl:TransitiveProperty ;
    rdfs:label "deliversTo" ;
    ofd:relatedService ofd:FoodDelivery ;
    rdfs:domain foaf:Agent ;
    rdfs:range foaf:Agent .

ofd:receivingFrom a skos:Concept, owl:TransitiveProperty ;
    rdfs:label "receivingFrom" ;
    ofd:relatedService ofd:FoodDistribution ;
    rdfs:domain foaf:Agent ;
    rdfs:range foaf:Agent .

ofd:storesFor a skos:Concept, owl:TransitiveProperty ;
    rdfs:label "storesFor" ;
    ofd:relatedService ofd:FoodStorage ;
    rdfs:domain foaf:Agent ;
    rdfs:range foaf:Agent .

ofd:pickingFor a skos:Concept, owl:TransitiveProperty ;
    rdfs:label "pickingFor" ;
    ofd:relatedService ofd:FoodPicking ;
    rdfs:domain foaf:Agent ;
    rdfs:range foaf:Agent .

ofd:packagingFor a skos:Concept, owl:TransitiveProperty ;
    rdfs:label "packagingFor" ;
    ofd:relatedService ofd:FoodPicking ;
    rdfs:domain foaf:Agent ;
    rdfs:range foaf:Agent .

ofd:hostingFor a skos:Concept, owl:TransitiveProperty ;
    rdfs:label "hostingFor" ;
    ofd:relatedService ofd:ServiceHosting ;
    rdfs:domain foaf:Agent ;
    rdfs:range foaf:Agent .

ofd:providesSoftwareFor a skos:Concept, owl:TransitiveProperty ;
    rdfs:label "providesSoftwareFor" ;
    ofd:relatedService ofd:SoftwareDelivery ;
    rdfs:domain foaf:Agent ;
    rdfs:range foaf:Agent .




# when it ts claimed
ofd:claimedBy a owl:ObjectProperty ;
    rdfs:label "claimedBy" ;
    rdfs:comment "Identifies the Agent who claimed ownership on this Food Actor and its data" ;
    rdfs:domain foaf:Agent ;
    rdfs:range foaf:Agent .

ofd:isLookingForPartners a owl:DataProperty ;
    rdfs:label "isLookingForPartners" ;
    rdfs:domain foaf:Agent ;
    rdfs:range xsd:boolean .

ofd:operationalCoverage a owl:ObjectProperty ;
    rdfs:label "operationalCoverage" ;
    rdfs:comment "activity coverage of a specific actor, which may depend on its agreements with other actors" ;
    rdfs:domain foaf:Agent ;
    rdfs:range schema:GeoShape .


# ##########################################
# base concepts

ofd:relatedService a owl:ObjectProperty ;
    rdfs:label "relatedService" ;
    rdfs:subPropertyOf skos:semanticRelation ;
    rdfs:comment "relates concept to a FoodService" ;
    rdfs:domain skos:Concept ;
    rdfs:range ofd:FoodServiceType .


# FoodActor

ofd:FoodActor a ofd:FoodActorType;
    skos:topConceptOf <https://w3id.org/openfooddata/core> .

ofd:Producer a ofd:FoodActorType;
    skos:broader:FoodActor ;
    rdfs:label "Producer" .

ofd:FoodHub a ofd:FoodActorType;
    skos:broader:FoodActor ;
    skos:broader:FoodActor ;
    rdfs:label "FoodHub" .

ofd:FoodCoop a ofd:FoodActorType;
    skos:broader:FoodActor ;
    rdfs:label "FoodCoop" .

# used for storing
ofd:FoodStore a ofd:FoodActorType;
    skos:broader:FoodActor ;
    rdfs:label "FoodStore" .

# performs delivery
ofd:FoodDistributor a ofd:FoodActorType;
    skos:broader:FoodActor ;
    rdfs:label "FoodDistributor" .

ofd:FoodPicker a ofd:FoodActorType;
    skos:broader:FoodActor ;
    rdfs:label "FoodPicker" .

ofd:SoftwareProvider a ofd:FoodActorType;
    skos:broader:FoodActor ;
    rdfs:label "Software Provider" .

ofd:FoodSAAS a ofd:FoodActorType;
    skos:broader:FoodActor ;
    rdfs:label "Food SAAS" .

# FoodService

ofd:FoodService a ofd:FoodServiceType;
    skos:topConceptOf <https://w3id.org/openfooddata/core> .

ofd:FoodProcessing a ofd:FoodServiceType;
    skos:broader:FoodService ;
    rdfs:label "Food Processing" .

ofd:FoodPicking a ofd:FoodServiceType;
    skos:broader:FoodService ;
    rdfs:label "Food Picking" .

ofd:FoodDistribution a ofd:FoodServiceType;
    skos:broader:FoodService ;
    rdfs:label "Food Distribution" .

ofd:FoodStorage a ofd:FoodServiceType;
    skos:broader:FoodService ;
    rdfs:label "Food Storage" .

ofd:ServiceHosting a ofd:FoodServiceType;
    skos:broader:FoodService ;
    rdfs:label "Service Hosting" .

ofd:SoftwareDelivery a ofd:FoodServiceType;
    skos:broader:FoodService ;
    rdfs:label "Software Delivery" .



# General Food types

ofd:Food a ofd:FoodType ;
    rdfs:label "Food" ;
    ofd:broaderAgrovoc agrovoc:Food ;
    ofd:broaderWikidata wikibase:Food ;
    skos:topConceptOf <https://w3id.org/openfooddata/core> .

ofd:Meat a ofd:FoodType ;
    rdfs:label "Meat" ;
    skos:broader:Food .

ofd:Fruit a ofd:FoodType ;
    rdfs:label "Fruit" ;
    skos:broader:Food .

ofd:Vegetable a ofd:FoodType ;
    rdfs:label "Vegetable" ;
    skos:broader:Food .

ofd:Fungi a ofd:FoodType ;
    rdfs:label "Fungi" ;
    skos:broader:Food .

ofd:NutAndSeed a ofd:FoodType ;
    rdfs:label "NutAndSeed" ;
    skos:broader:Food .

ofd:DairyProduct a ofd:FoodType ;
    rdfs:label "DairyProduct" ;
    skos:broader:Food .

ofd:Seafood a ofd:FoodType ;
    rdfs:label "Seafood" ;
    skos:broader:Food .

ofd:OilsAndFats a ofd:FoodType ;
    rdfs:label "Oils and Fats" ;
    skos:broader:Food .

ofd:Egg a ofd:FoodType ;
    rdfs:label "Egg" ;
    skos:broader:Food .

ofd:BakedFood a ofd:FoodType ;
    rdfs:label "BakedFood" ;
    skos:broader:Food .

ofd:Drink a ofd:FoodType ;
    rdfs:label "Drink" ;
    skos:broader:Food .

ofd:Cereals a ofd:FoodType ;
    rdfs:label "Cereals" ;
    skos:broader:Food .

